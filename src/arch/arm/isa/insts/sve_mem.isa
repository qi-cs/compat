    StaticInstPtr
    decodeSveContigConseLoadSIInsts(uint8_t msz, ExtMachInst machInst,
            RegIndex zt, RegIndex pg, RegIndex rn,
            int64_t imm, int numregs, bool nonTemporal)
    {
        static const char* nm[2][4] = {
            { "ld1b", "ld1h", "ld1w", "ld1d" },
            { "ldnt1b", "ldnt1h", "ldnt1w", "ldnt1d" } };

        if (!nonTemporal) {
            switch (msz) {
                case 0:
                    return new SveLdContigConseSI<uint8_t,
                            SveLoadRegImmPngMicroop>(
                                nm[nonTemporal][msz], machInst, MemReadOp,
                                zt, pg, rn, imm, numregs);
                case 1:
                    return new SveLdContigConseSI<uint16_t,
                            SveLoadRegImmPngMicroop>(
                                nm[nonTemporal][msz], machInst, MemReadOp,
                                zt, pg, rn, imm, numregs);
                case 2:
                    return new SveLdContigConseSI<uint32_t,
