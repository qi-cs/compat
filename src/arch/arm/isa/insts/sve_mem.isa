            {'targs': 'Element',
             'memacc_code': loadMemAccCode,
             'ea_code' : (sveEnabledCheckCode + eaCode),
             'fa_code' : load_fa_code,
             'auxgp_code' : auxgpCode},
            ['IsLoad', 'IsMicroop'])
        storeIop = ArmInstObjParams('stxx',
            'SveStoreRegImmQuadMicroop' if offsetIsImm
                                        else 'SveStoreRegRegQuadMicroop',
            'MicroOp',
            {'targs': 'Element',
             'wren_code': storeWrEnableCode,
             'memacc_code': storeMemAccCode,
             'ea_code' : (sveEnabledCheckCode + eaCode),
             'fa_code' : store_fa_code,
             'auxgp_code' : auxgpCode},
            ['IsStore', 'IsMicroop'])
        if offsetIsImm:
            header_output += SveStructMemSIMicroopDeclare.subst(loadIop)
            header_output += SveStructMemSIMicroopDeclare.subst(storeIop)
        else:
            header_output += SveStructMemSSMicroopDeclare.subst(loadIop)
            header_output += SveStructMemSSMicroopDeclare.subst(storeIop)
        exec_output += (
            SveStructLoadExecute.subst(loadIop) +
            SveStructLoadInitiateAcc.subst(loadIop) +
            SveStructLoadCompleteAcc.subst(loadIop) +
            SveStructStoreExecute.subst(storeIop) +
            SveStructStoreInitiateAcc.subst(storeIop) +
            SveStructStoreCompleteAcc.subst(storeIop))
        tplArgs = ('__uint128_t',)
        for type in tplArgs:
            substDict = {'targs': type,
                         'class_name': ('SveLoadRegImmQuadMicroop'
                                        if offsetIsImm
                                        else 'SveLoadRegRegQuadMicroop')}
            exec_output += SveStructMemExecDeclare.subst(substDict)
            substDict['class_name'] = ('SveStoreRegImmQuadMicroop'
                                       if offsetIsImm
                                       else 'SveStoreRegRegQuadMicroop')
            exec_output += SveStructMemExecDeclare.subst(substDict)

    # Generates definitions for SVE consecutive load/store microops
    def emitSveConseMemInsts(offsetIsImm):
        global header_output, exec_output, decoders
        eaCode = SPAlignmentCheckCode + '''
        int memAccessSize = eCount * sizeof(Element);
        EA = memAccessSize * regIndex + XBase + '''
        if offsetIsImm:
            eaCode += '((int64_t) this->imm * eCount * sizeof(Element))'
        else:
            eaCode += '(XOffset * sizeof(Element));'
        pngDecoder = '''
        uint16_t png = 0;
        for (unsigned b = 0; b < 16; b ++) {
            png = png | ((uint16_t)GpOp_ub[b] << b);
        }
        sveCounterToPredicate(
            auxGpReg, png, sizeof(Element) * eCount, 0, regIndex);
        '''
        loadMemAccCode = '''
        for (int i = 0; i < eCount; i++) {
            if (auxGp[i]) {
                AA64FpDest_x[i] = cSwap<Element>(memDataView[i],
                                        isBigEndian64(xc->tcBase()));
            } else {
                AA64FpDest_x[i] = 0;
            }
        }
        '''
        storeMemAccCode = '''
        for (int i = 0; i < eCount; i++) {
            if (auxGp[i]) {
                memDataView[i] = cSwap<Element>(AA64FpDest_x[i],
                                        isBigEndian64(xc->tcBase()));
            } else {
                memDataView[i] = 0;
                for (int j = 0; j < sizeof(Element); j++) {
                    wrEn[sizeof(Element) * i + j] = false;
                }
            }
        }
        '''
        storeWrEnableCode = '''
        auto wrEn = std::vector<bool>(sizeof(Element) * eCount, true);
        '''
        loadIop = ArmInstObjParams('ldxx',
            ('SveLoadRegImmPngMicroop' if offsetIsImm else
             'SveLoadRegRegPngMicroop'),
            'MicroOp',
            {'targs': 'Element',
             'memacc_code': loadMemAccCode,
             'ea_code' : (sveEnabledCheckCode + eaCode),
             'fa_code' : load_fa_code,
             'auxgp_code': pngDecoder},
            ['IsLoad', 'IsMicroop'])
        loadNTIop = ArmInstObjParams('ldntxx',
            ('SveLoadRegImmPngNTMicroop' if offsetIsImm else
             'SveLoadRegRegPngNTMicroop'),
            'MicroOp',
            {'targs': 'Element',
             'memacc_code': loadMemAccCode,
             'ea_code' : (sveEnabledCheckCode + eaCode),
             'fa_code' : load_fa_code,
             'auxgp_code': pngDecoder},
            ['IsLoad', 'IsMicroop'])
        storeIop = ArmInstObjParams('stxx',
            ('SveStoreRegImmPngMicroop' if offsetIsImm else
             'SveStoreRegRegPngMicroop'),
            'MicroOp',
            {'targs': 'Element',
             'wren_code': storeWrEnableCode,
             'memacc_code': storeMemAccCode,
             'ea_code' : (sveEnabledCheckCode + eaCode),
             'fa_code' : store_fa_code,
             'auxgp_code': pngDecoder},
            ['IsStore', 'IsMicroop'])
        storeNTIop = ArmInstObjParams('stntxx',
            ('SveStoreRegImmPngNTMicroop' if offsetIsImm else
             'SveStoreRegRegPngNTMicroop'),
            'MicroOp',
            {'targs': 'Element',
             'wren_code': storeWrEnableCode,
             'memacc_code': storeMemAccCode,
             'ea_code' : (sveEnabledCheckCode + eaCode),
             'fa_code' : store_fa_code,
             'auxgp_code': pngDecoder},
            ['IsStore', 'IsMicroop'])
        if offsetIsImm:
            header_output += SveStructMemSIMicroopDeclare.subst(loadIop)
            header_output += SveStructMemSIMicroopDeclare.subst(loadNTIop)
            header_output += SveStructMemSIMicroopDeclare.subst(storeIop)
            header_output += SveStructMemSIMicroopDeclare.subst(storeNTIop)
        else:
            header_output += SveStructMemSSMicroopDeclare.subst(loadIop)
            header_output += SveStructMemSSMicroopDeclare.subst(loadNTIop)
            header_output += SveStructMemSSMicroopDeclare.subst(storeIop)
            header_output += SveStructMemSSMicroopDeclare.subst(storeNTIop)
        exec_output += (
            SveStructLoadExecute.subst(loadIop) +
            SveStructLoadInitiateAcc.subst(loadIop) +
            SveStructLoadCompleteAcc.subst(loadIop) +
            SveStructLoadExecute.subst(loadNTIop) +
            SveStructLoadInitiateAcc.subst(loadNTIop) +
            SveStructLoadCompleteAcc.subst(loadNTIop) +
            SveStructStoreExecute.subst(storeIop) +
            SveStructStoreInitiateAcc.subst(storeIop) +
            SveStructStoreCompleteAcc.subst(storeIop) +
            SveStructStoreExecute.subst(storeNTIop) +
            SveStructStoreInitiateAcc.subst(storeNTIop) +
            SveStructStoreCompleteAcc.subst(storeNTIop))
        tplArgs = ('uint8_t', 'uint16_t', 'uint32_t', 'uint64_t')
        for type in tplArgs:
            substDict = {'targs': type,
                         'class_name': 'SveLoadRegImmPngMicroop'
                                        if offsetIsImm
                                        else 'SveLoadRegRegPngMicroop'}
            exec_output += SveStructMemExecDeclare.subst(substDict)
            substDict['class_name'] = ('SveLoadRegImmPngNTMicroop'
                                       if offsetIsImm
                                       else 'SveLoadRegRegPngNTMicroop')
            exec_output += SveStructMemExecDeclare.subst(substDict)
            substDict['class_name'] = ('SveStoreRegImmPngMicroop'
                                       if offsetIsImm
                                       else 'SveStoreRegRegPngMicroop')
            exec_output += SveStructMemExecDeclare.subst(substDict)
            substDict['class_name'] = ('SveStoreRegImmPngNTMicroop'
                                       if offsetIsImm
                                       else 'SveStoreRegRegPngNTMicroop')
            exec_output += SveStructMemExecDeclare.subst(substDict)

    # Generates definitions for SVE load-and-replicate quadword instructions
    def emitSveLoadAndReplMulti(offsetIsImm, numQwordSegments):
        global header_output, exec_output, decoders
        assert(numQwordSegments in (1, 2)) # Quadword or Octaword
        from collections import namedtuple
        InstConfig = namedtuple("_InstConfig", "mnemonic classname baseclass")
        INST_CONFIGURATIONS = {
            # (offsetIsImm, numQwordSegments) -> InstConfig Recors
            (True, 1): InstConfig("ld1rq", "SveLd1RqSI", "SveContigMemSI"),
            (False, 1): InstConfig("ld1rq", "SveLd1RqSS", "SveContigMemSS"),
            (True, 2): InstConfig("ld1ro", "SveLd1RoSI", "SveContigMemSI"),
            (False, 2): InstConfig("ld1ro", "SveLd1RoSS", "SveContigMemSS"),
        }
        inst_config = INST_CONFIGURATIONS[(offsetIsImm, numQwordSegments)]
        memAccessSize = numQwordSegments * 16;
        tplHeader = 'template <class RegElemType, class MemElemType>'
        tplArgs = '<RegElemType, MemElemType>'
        eaCode = SPAlignmentCheckCode + '''
        int memAccessSize = %(memAccessSize)d;
        EA = XBase + ''' % dict(memAccessSize=memAccessSize)
        if offsetIsImm:
            eaCode += ('(((int64_t) this->imm) * %(memAccessSize)d);'
                       % dict(memAccessSize=memAccessSize))
        else:
            eaCode += '(XOffset * sizeof(MemElemType));'
        loadRdEnableCode = '''
        eCount = %(memAccessSize)d/sizeof(RegElemType);
        auto rdEn = std::vector<bool>(%(memAccessSize)d, true);
        for (int i = 0; i < eCount; ++i) {
            if (!GpOp_x[i]) {
                for (int j = 0; j < sizeof(RegElemType); ++j) {
                    rdEn[sizeof(RegElemType) * i + j] = false;
                }
            }
        }
        ''' % dict(memAccessSize=memAccessSize)
        memAccCode = '''
        // Copy active elements of the data from memory into a temporary
        // quadword/octaword
        __uint128_t qwords[%(numQwordSegments)d];
        eCount = %(memAccessSize)d/sizeof(RegElemType);
        RegElemType* qp = reinterpret_cast<RegElemType*>(&qwords);
        for (int i = 0; i < eCount; ++i) {
            if (GpOp_x[i]) {
                qp[i] = cSwap<RegElemType>(memDataView[i],
                                            isBigEndian64(xc->tcBase()));

            } else {
                qp[i] = 0;
            }
        }
        // Repeat the temporary quadword/octaword segment into the
        // vector register. Zero fill the remainder for non-full
        // octawords.
        unsigned numQwords = ArmStaticInst::getCurSveVecLen<__uint128_t>(
                xc->tcBase());
        unsigned numFullQwords = numQwords -
                                 (numQwords %% %(numQwordSegments)d);
        for (int i = 0; i < numQwords; ++i) {
            if (i < numFullQwords) {
                AA64FpDest_uq[i] = qwords[i %% %(numQwordSegments)d];
            } else {
                AA64FpDest_uq[i] = 0;
            }
        }
        ''' % dict(memAccessSize=memAccessSize,
                   numQwordSegments=numQwordSegments)
        iop = ArmInstObjParams(
                inst_config.mnemonic,
                inst_config.classname,
                inst_config.baseclass,
                {'tpl_header': tplHeader,
                 'tpl_args': tplArgs,
                 'rden_code': loadRdEnableCode,
                 'memacc_code': memAccCode,
                 'ea_code': (sveEnabledCheckCode + eaCode),
                 'fault_code': '',
                 'fa_code': load_fa_code},
                ['IsLoad'])
        if offsetIsImm:
            header_output += SveContigMemSIOpDeclare.subst(iop)
        else:
            header_output += SveContigMemSSOpDeclare.subst(iop)
        exec_output += (
                SveContigLoadExecute.subst(iop) +
                SveContigLoadInitiateAcc.subst(iop) +
                SveContigLoadCompleteAcc.subst(iop))
        for ttype in ('uint8_t', 'uint16_t', 'uint32_t', 'uint64_t'):
            substDict = {'tpl_args': '<%s, %s>' % (ttype, ttype),
                         'class_name': inst_config.classname}
            exec_output += SveContigMemExecDeclare.subst(substDict)

    # LD1[S]{B,H,W,D} (scalar plus immediate)
    # LDNT1[S]{B,H,W,D} (scalar plus immediate)
    # ST1[S]{B,H,W,D} (scalar plus immediate)
    # STNT1[S]{B,H,W,D} (scalar plus immediate)
    # LDNF1[S]{B,H,W,D} (scalar plus immediate)
    emitSveContigMemInsts(True)
    # LD1[S]{B,H,W,D} (scalar plus scalar)
    # LDNT1[S]{B,H,W,D} (scalar plus scalar)
    # ST1[S]{B,H,W,D} (scalar plus scalar)
    # STNT1[S]{B,H,W,D} (scalar plus scalar)
    # LDFF1[S]{B,H,W,D} (scalar plus scalar)
    emitSveContigMemInsts(False)

    # LD1{W,D} (scalar plus immediate, quad)
    # ST1{W,D} (scalar plus immediate, quad)
    emitSveContigMemQuadInsts(True)
    # LD1{W,D} (scalar plus scalar, quad)
    # ST1{W,D} (scalar plus scalar, quad)
    emitSveContigMemQuadInsts(False)

    # LD1R[S]{B,H,W,D}
    emitSveLoadAndRepl()

    # LD1RQ{B,H,W,D} (scalar plus immediate)
    emitSveLoadAndReplMulti(offsetIsImm=True, numQwordSegments=1)
    # LD1RQ{B,H,W,D} (scalar plus scalar)
    emitSveLoadAndReplMulti(offsetIsImm=False, numQwordSegments=1)

    # LD1RO{B,H,W,D} (scalar plus immediate)
    emitSveLoadAndReplMulti(offsetIsImm=True, numQwordSegments=2)
    # LD1RO{B,H,W,D} (scalar plus scalar)
    emitSveLoadAndReplMulti(offsetIsImm=False, numQwordSegments=2)

    # LD{2,3,4}{B,H,W,D} (scalar plus immediate)
    # ST{2,3,4}{B,H,W,D} (scalar plus immediate)
    emitSveStructMemInsts(offsetIsImm = True)
    # LD{2,3,4}{B,H,W,D} (scalar plus scalar)
    # ST{2,3,4}{B,H,W,D} (scalar plus scalar)
    emitSveStructMemInsts(offsetIsImm = False)

    # LD{2,3,4}{Q} (scalar plus immediate)
    # ST{2,3,4}{Q} (scalar plus immediate)
    emitSveStructMemQuadInsts(offsetIsImm = True)
    # LD{2,3,4}{Q} (scalar plus scalar)
    # ST{2,3,4}{Q} (scalar plus scalar)
    emitSveStructMemQuadInsts(offsetIsImm = False)

    # LD1{B,H,W,D} {scalar plus immediate, consecutive registers}
    # LDNT1{B,H,W,D} {scalar plus immediate, consecutive registers}
    # ST1{B,H,W,D} {scalar plus immediate, consecutive registers}
    # STNT1{B,H,W,D} {scalar plus immediate, consecutive registers}
    emitSveConseMemInsts(offsetIsImm = True)
    # LD1{B,H,W,D} {scalar plus scalar, consecutive registers}
    # LDNT1{B,H,W,D} {scalar plus scalar, consecutive registers}
    # ST1{B,H,W,D} {scalar plus scalar, consecutive registers}
    # STNT1{B,H,W,D} {scalar plus scalar, consecutive registers}
    emitSveConseMemInsts(offsetIsImm = False)

    # LDR (predicate), STR (predicate)
    emitSveMemFillSpill(True)
    # LDR (vector), STR (vector)
    emitSveMemFillSpill(False)

    # LD1[S]{B,H,W,D} (vector plus immediate)
    # ST1[S]{B,H,W,D} (vector plus immediate)
    # LDFF1[S]{B,H,W,D} (scalar plus immediate)
    emitSveIndexedMemMicroops(IndexedAddrForm.VEC_PLUS_IMM)
    # LD1[S]{B,H,W,D} (scalar plus vector)
    # ST1[S]{B,H,W,D} (scalar plus vector)
    # LDFF1[S]{B,H,W,D} (scalar plus vector)
    emitSveIndexedMemMicroops(IndexedAddrForm.SCA_PLUS_VEC)
    # LDNT1[S]{B,H,W,D} (vector plus scalar)
    # STNT1[S]{B,H,W,D} (vector plus scalar)
    emitSveIndexedMemMicroops(IndexedAddrForm.VEC_PLUS_SCA)
    # LD1Q
    # ST1Q
    emitSveIndexedMemQuadMicroops(IndexedAddrForm.VEC_PLUS_SCA)

    # FFR writeback microop for gather loads
    emitSveFirstFaultWritebackMicroop()

    # Source vector copy microop for gather loads
    emitSveGatherLoadCpyDstVecMicroop()

    # ST/LD struct de/interleave microops
    emitSveInterleaveMicroop()
    emitSveDeInterleaveMicroop()
}};
